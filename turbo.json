{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//root"],
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "cache": false
    },
    "format": {},
    "format:fix": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "publish-packages": {
      "cache": false,
      "persistent": true
    }
  }
}
